<div class="content-holder clearfix">
  <div class="container content-zone about-page">
    <div class="row">
      <div class="col-sm-7">
        {{website_data.about_text}}
      </div>
      <div class="col-sm-5">
        {% for photo in website_data.about_photos limit: 2 offset: 0 %}
          <img class="about-photo" src="{{ photo.image | attachment_url: 'public_big' }}" />
        {% endfor %}
      </div>
    </div>
  </div>
</div>
